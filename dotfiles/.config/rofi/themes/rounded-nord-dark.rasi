/* ROUNDED THEME FOR ROFI */
/* Author: Newman Sanchez (https://github.com/newmanls) */
configuration {
    show-icons: true;
    icon-theme: "Papirus-Dark";
}

* {
    bg0:    #1e1e2e;
    bg1:    #1e1e2e;
    bg2:    #1e1e2e;
    bg3:    #FFFFFF;
    fg0:    #D8DEE9;
    fg1:    #ECEFF4;
    fg2:    #D8DEE9;
    fg3:    #4C566A;
}

@import "template/rounded-template.rasi"

element selected {
    text-color: @bg1;
}
